function graphDistLine(x, y, name, color)
    txt = "%s : y = %4.2fx ";
    %txttwo = "%s ?:
    hold on
    %[p, delta] = polyfix(x, y, 1, 0, 0);
    %legend = sprintf(txt, name, p(1,1));
    [p,~,mu] = polyfit(x,y,1);
    hold on;
    scatter(t1(1,:), t1LetterHeights(1,:), 15, "m", "filled");
    %plot(x,y,'.','Color', color, 'HandleVisibility', 'off');
    %plot(x,polyval(p,x), 'Color', color, 'LineWidth', 1, 'DisplayName', sprintf(txt, name, p(1,1)));
    %plot(x,polyval(p,x,delta), 'Color', color, 'LineWidth', 1, 'DisplayName', sprintf(txt, name, p(1,1)));
end